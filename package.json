{
    "name": "properweb",
    "version": "1.0.7",
    "description": "",
    "main": "./dist/properweb.js",
    "types": "./index.d.ts",
    "scripts": {
        "build": "npm-run-all --parallel build:*",
        "build:core-cjs": "esbuild ./index.js --bundle --minify --outfile=./dist/properweb.js --inject:./document-shim.js --platform=node --format=cjs --allow-overwrite",
        "build:core-esm": "esbuild ./index.js --bundle --minify --outfile=./dist/properweb.mjs --format=esm --allow-overwrite"
    },
    "exports": {
        ".": {
            "node": "./dist/properweb.js",
            "require": "./dist/properweb.js",
            "import": "./dist/properweb.mjs"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint": "^7.27.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "sade": "^1.7.4",
        "typescript": "^4.3.2"
    },
    "devDependencies": {
        "esbuild": "^0.12.9",
        "jsdom": "^16.6.0",
        "npm-run-all": "^4.1.5"
    }
}
